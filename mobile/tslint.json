{
  "defaultSeverity": "error",
  "extends": [
    "tslint:all",
    "tslint-react"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules"
    ]
  },
  "rules": {
    "completed-docs": [
      true,
      "interfaces"
    ],
    "no-implicit-dependencies": [
      true,
      "dev"
    ]
  }
}
