{
    "defaultSeverity": "error",
    "extends": [
        "tslint:all",
        "tslint-react"
    ],
    "cliOptions": {
        "exclude": [
            "scenes/**/*.js",
            "scenes/**/*.css.d.ts",
            "node_modules"
        ]
    },
    "jsRules": {},
    "rules": {
        "no-implicit-dependencies": [
            true,
            "dev"
        ],
        "no-unsafe-any": false,
        "no-magic-numbers": false
    },
    "rulesDirectory": []
}
