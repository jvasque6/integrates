  "scripts": {
    "audit": "npm set audit-level high && npm config set audit-level high && npm audit",
    "build": "npm run css && webpack --hide-modules --config webpack.dev.config.js",
    "build:prod": "webpack --hide-modules --config webpack.prod.config.js",
    "css": "tcm src/ --silent",
    "lint": "tslint -p tsconfig.json -t codeFrame",
    "test": "tsc -p tsconfig.json --noEmit && npm run css && ./runTests.sh"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "src/**/*.spec.tsx"
    ],
    "reporter": [
      "text-summary"
    ],
    "all": true
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "text", "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!**/node_modules/**",
      "!**/*.d.ts",
      "!src/**/*.spec.tsx"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "coverageDirectory": "coverage",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transformIgnorePatterns": [
      "node_modules/*"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testRegex": ["src/components/ConfirmDialog/index.spec.tsx", "src/components/DataTable/index.spec.tsx",
      "src/components/Button/index.spec.tsx"],
    "preset": "ts-jest",
    "verbose": true
  }
